<% provide(:title, "Pickup") %>
<div class="row">
	<div class="col-sm-4 col-sm-offset-4">
		<%= form_tag pickups_path do %>
			<%= number_field_tag :pickup_code, nil, class: 'form-control', placeholder: "Enter 4-digit pickup code here..." %>
			<%= submit_tag "Enter", class: 'btn btn-lg btn-primary form-control' %>
		<% end %>
	</div>
</div>